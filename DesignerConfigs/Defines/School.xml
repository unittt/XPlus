<module name="SchoolModule">

<enum name="ESchoolType">
	<var name="ShuMountain" alias="蜀山" comment="蜀山"/>
	<var name="GoldenMountainTemple" alias="金山寺" comment="金山寺"/>
	<var name="TaiChu"  alias="太初" comment="太初"/>
	<var name="Yaochi"  alias="瑶池" comment="瑶池"/>
	<var name="Qingcheng"  alias="青城" comment="青城"/>
	<var name="DemonGodPalace"  alias="妖神宫" comment="妖神宫"/>
</enum>


<bean name="School">
		<var name="SchoolType" type="ESchoolType" comment="门派类型"/>
		<var name="Name" type="string" comment="门派名字"/>
		<var name="Desc" type="string" comment="描述"/>
	</bean>
	<table name="TbSchool" value="School" index="SchoolType" input="system/门派.xlsx"/>
</module>